{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "resources": [
    {
      "type": "Microsoft.Network/virtualNetworks/subnets",
      "apiVersion": "2023-05-01",
      "name": "vn01-east-us-aj/sn-dmz-east-us-aj",
      "properties": {
        "addressPrefix": "172.21.21.0/24",
        "routeTable": {
          "id": "[resourceId('Microsoft.Network/routeTables', 'rt-sn-dmz-east-us-aj')]"
        }
      }
    },
    {
      "type": "Microsoft.Network/virtualNetworks/subnets",
      "apiVersion": "2023-05-01",
      "name": "vn01-east-us-aj/sn-server-lan-east-us-aj",
      "properties": {
        "addressPrefix": "172.21.22.0/24",
        "routeTable": {
          "id": "[resourceId('Microsoft.Network/routeTables', 'rt-sn-server-lan-east-us-aj')]"
        }
      }
    },
    {
      "type": "Microsoft.Network/virtualNetworks/subnets",
      "apiVersion": "2023-05-01",
      "name": "vn01-east-us-aj/sn-access-lan-east-us-aj",
      "properties": {
        "addressPrefix": "172.21.23.0/24",
        "routeTable": {
          "id": "[resourceId('Microsoft.Network/routeTables', 'rt-sn-access-lan-east-us')]"
        }
      }
    }
  ]
}
